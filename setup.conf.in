# Configuration du script de setup

# setup
SETUP_MODULES=@LIBEXECDIR@/vigilo/setup
export SETUP_MODULES

# vigiconf
export VIGICONF_SVN=file:///var/lib/svn/vigiconf
export VIGICONF_SVN

# Emplacement des RPMs
VIGILO_REPO=http://vigilo-dev.si.c-s.fr/pkg/$DISTRO/nightly/master/vigilo/vigilo/$DIST_TAG/`uname -m`
VIGILO_DEPS_REPO=http://vigilo-dev.si.c-s.fr/pkg/$DISTRO/deps/vigilo/$DIST_TAG/`uname -m`
VIGILO_EXTRA_REPO=http://vigilo-dev.si.c-s.fr/pkg/$DISTRO/deps-epel/vigilo/$DIST_TAG/`uname -m`
export VIGILO_REPO
export VIGILO_DEPS_REPO
export VIGILO_EXTRA_REPO

# Emplacement du fichier pg_hba.conf
# de la base PostgreSQL Ã  utiliser.
PG_HBA=/var/lib/pgsql/data/pg_hba.conf
export PG_HBA
